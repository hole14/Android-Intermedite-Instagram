<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/instagram_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.myinstagram.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <activity
        android:id="@+id/searchFragment"
        android:name="com.example.myinstagram.fragment.SearchFragment"
        android:label="activity_search_fragment"
        tools:layout="@layout/activity_search_fragment" />
    <fragment
        android:id="@+id/unggahFragment"
        android:name="com.example.myinstagram.fragment.UnggahFragment"
        android:label="fragment_unggah"
        tools:layout="@layout/fragment_unggah" />
    <fragment
        android:id="@+id/reelsFragment"
        android:name="com.example.myinstagram.fragment.ReelsFragment"
        android:label="fragment_reels"
        tools:layout="@layout/fragment_reels" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.myinstagram.fragment.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>